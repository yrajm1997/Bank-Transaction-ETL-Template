{
    "name": "Python ETL Environment",
    "image": "mcr.microsoft.com/vscode/devcontainers/python:3.9",
    "features": {},
    "extensions": [
        "ms-python.python",
        "ms-azuretools.vscode-docker"
    ],
    "settings": {
      "python.pythonPath": "/usr/local/bin/python"
    },
    "postCreateCommand": "pip install -r requirements.txt"
  }